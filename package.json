{
  "name": "demo",
  "description": "This app contains demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "develop": "nodemon server.js",
    "debug": "nodemon --inspect=9231 server.js",
    "v8Profile": "node --prof server.js",
    "standard": "standard --fix"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "watch": [
      "app_modules",
      "routes",
      "workers"
    ],
    "events": {
      "restart": "fuser -k 3000/tcp;",
      "start": "fuser -k 3000/tcp;",
      "crash": "fuser -k 3000/tcp;"
    },
    "delay": "1500"
  },
  "pre-commit": [
    "standard"
  ],
  "standard": {
    "ignore": [
      "/lib/"
    ]
  },
  "dependencies": {
    "aws-sdk": "2.682.0",
    "cors": "2.8.5",
    "date-format-utils": "^2.2.0",
    "deep-email-validator": "^0.1.21",
    "dns": "^0.2.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-request-id": "1.4.1",
    "firebase-admin": "^11.10.1",
    "fs": "0.0.1-security",
    "helmet": "3.20.0",
    "http-errors": "1.7.3",
    "humanize-plus": "1.8.2",
    "jsonschema": "1.2.6",
    "jsonwebtoken": "8.5.1",
    "jwt-simple": "0.5.5",
    "lodash": "4.17.15",
    "moment": "2.25.3",
    "moment-timezone": "0.5.26",
    "mongodb": "^3.1.12",
    "mongodb-autoincrement": "^1.0.1",
    "multer": "1.4.2",
    "multer-s3": "2.9.0",
    "mysql2": "^3.2.0",
    "nodemailer": "6.4.6",
    "nodemailer-smtp-transport": "2.7.4",
    "passport": "0.4.0",
    "passport-facebook": "3.0.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "path": "^0.12.7",
    "q": "^1.5.1",
    "qrcode": "1.4.4",
    "rate-limiter-flexible": "2.1.10",
    "redis": "2.8.0",
    "request": "2.88.2",
    "speakeasy": "2.0.0",
    "stripe": "^12.17.0",
    "swagger-ui-express": "^4.6.2",
    "util": "^0.12.4",
    "uuid4": "1.1.4",
    "validator": "^13.9.0",
    "winston": "3.1.0",
    "winston-daily-rotate-file": "3.10.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "nodemon": "2.0.2",
    "pre-commit": "^1.2.2",
    "standard": "14.3.3"
  }
}